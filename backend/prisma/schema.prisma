}

model mPanel {
  sPanelID String @id @default(uuid())
  sName    String @default("")
  sMaterialName    String @default("")
  nLength  Int    @default(0)
  nWidth   Int    @default(0)
  nQty     Int    @default(0)
}

model mUserNew {
  sUserID     String     @id @default(uuid())
  sName       String
  sUserName   String
  sPassword   String
